<div class="center-container">
  <h2 class="pt-5">Add a memory</h2>
    <div class="form-signup">
      <%= simple_form_for [@user_gig, @comment] do |f|%>
        <%= f.input :content, label: 'Comment' %>
        <%= f.input :rating %>
        <%= f.input :photo, as: :file  %>
        <%= f.submit "Upload memories", class: "btn primary-btn" %>
      <% end %>
    </div>
  </div>
